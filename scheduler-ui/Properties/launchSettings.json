{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Angular Dev Server": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoExit -Command \"npm start\"",
      "workingDirectory": ".",
      "environmentVariables": {
        "NODE_ENV": "development"
      },
      "launchBrowser": true,
      "launchUrl": "http://localhost:4200"
    },
    "Angular Dev Server (No Browser)": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoExit -Command \"npm start\"",
      "workingDirectory": ".",
      "environmentVariables": {
        "NODE_ENV": "development"
      },
      "launchBrowser": false
    },
    "npm install": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoExit -Command \"npm install\"",
      "workingDirectory": ".",
      "environmentVariables": {
        "NODE_ENV": "development"
      },
      "launchBrowser": false
    }
  }
}
